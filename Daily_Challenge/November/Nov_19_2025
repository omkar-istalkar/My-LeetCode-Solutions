class Solution:
    
    out = 0
    
    def check(self, arr, num):
        if num in arr:
            num = num * 2
            print(f"New Original : {num}")
            self.out = num
            self.check(arr, num)
        
    
    
    def findFinalValue(self, nums: List[int], original: int) -> int:
        self.check(nums, original)
        return max(original, self.out)
        
