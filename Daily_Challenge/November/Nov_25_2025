class Solution:
    def smallestRepunitDivByK(self, k: int) -> int:
        curr = 1
        out = 1
        prev = set()
        
        while curr % k:
            print(curr, curr % k)
            if curr in prev:
                return -1
            prev.add(curr)
            curr  = (10 * (curr % k)) + 1
            out += 1
            
        print(prev)
            
        return out
        
sol = Solution()
print(sol.smallestRepunitDivByK(k = 3))
